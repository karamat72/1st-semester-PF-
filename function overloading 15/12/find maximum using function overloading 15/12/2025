#include <iostream>
using namespace std;

// Function with 2 parameters
void mix(int a, int b)
{
    if (a > b)
    {
        cout << "A is largest: " << a << endl;
    }
    else
    {
        cout << "B is largest: " << b << endl;
    }
}

// Function with 3 parameters
void mix(int x, int y, int z)
{
    if (x > y && x > z)
    {
        cout << "X is maximum: " << x << endl;
    }
    else if (y > x && y > z)
    {
        cout << "Y is maximum: " << y << endl;
    }
    else
    {
        cout << "Z is maximum: " << z << endl;
    }
}

int main()
{
    mix(1, 3);      // Calls 2-argument function
    mix(2, 4, 6);   // Calls 3-argument function

    return 0;
}
